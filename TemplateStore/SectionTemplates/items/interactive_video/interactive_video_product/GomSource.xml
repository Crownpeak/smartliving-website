<?xml version="1.0" encoding="UTF-8"?>
<CMS_MODULE>

  <CMS_INPUT_NUMBER name="st_time" type="double" allowEmpty="no" hFill="no" min="0.0" singleLine="yes" useLanguages="no">
    <LANGINFOS>
      <LANGINFO lang="*" label="Second of appearance:" format="0.000"/>
      <LANGINFO lang="DE" label="Anzeigesekunde:" format="0.000"/>
    </LANGINFOS>
  </CMS_INPUT_NUMBER>

  <FS_DATASET name="st_product" hFill="yes" useLanguages="no">
    <LANGINFOS>
      <LANGINFO lang="*" label="Product"/>
      <LANGINFO lang="DE" label="Produkt"/>
    </LANGINFOS>
    <SOURCES>
      <CONTENT name="product"/>
    </SOURCES>
  </FS_DATASET>

</CMS_MODULE>