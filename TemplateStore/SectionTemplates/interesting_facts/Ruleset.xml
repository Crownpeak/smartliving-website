<?xml version="1.0" encoding="UTF-8"?>
<RULES>
	<!-- Hide New-Button at 3 elements: st_counters -->
	<RULE>
		<WITH>
			<LESS_THAN>
				<PROPERTY name="SIZE" source="st_counters"/>
				<NUMBER>3</NUMBER>
			</LESS_THAN>
		</WITH>
		<DO>
			<PROPERTY name="NEW" source="st_counters"/>
		</DO>
	</RULE>
</RULES>