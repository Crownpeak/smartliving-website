<?xml version="1.0" encoding="UTF-8"?>
<CMS_MODULE>

  <CMS_INPUT_NUMBER name="st_time" type="double" allowEmpty="no" hFill="no" min="0.0" singleLine="yes" useLanguages="no">
    <LANGINFOS>
      <LANGINFO lang="*" label="Second of appearance:" format="0.000"/>
    </LANGINFOS>
  </CMS_INPUT_NUMBER>

  <CMS_INPUT_DOM
    name="st_text"
    allowEmpty="no"
    bold="yes"
    hFill="yes"
    italic="yes"
    list="yes"
    table="no"
    useLanguages="yes">
    <FORMATS>
      <TEMPLATE name="p"/>
    </FORMATS>
    <LANGINFOS>
      <LANGINFO lang="*" label="Text"/>
    </LANGINFOS>
    <LINKEDITORS>
      <LINKEDITOR name=""/>
    </LINKEDITORS>
  </CMS_INPUT_DOM>

</CMS_MODULE>