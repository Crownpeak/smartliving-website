<?xml version="1.0" encoding="UTF-8"?>
<RULES>
	<RULE>
		<WITH>
			<PROPERTY name="EMPTY" source="pt_key"/>
		</WITH>
		<DO>
			<NOT>
				<VALIDATION scope="RELEASE">
					<PROPERTY name="VALID" source="pt_key"/>
					<MESSAGE lang="*" text="The key must not be empty."/>
				</VALIDATION>
			</NOT>
		</DO>
	</RULE>
</RULES>