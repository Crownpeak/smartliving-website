<?xml version="1.0" encoding="UTF-8"?>
<CMS_MODULE>

  <CMS_INPUT_TEXT name="tt_name" hFill="yes" singleLine="no">
    <LANGINFOS>
      <LANGINFO lang="*" label="Name"/>
    </LANGINFOS>
  </CMS_INPUT_TEXT>

  <FS_REFERENCE name="tt_pin" hFill="yes" useLanguages="no">
    <FILTER>
      <ALLOW type="picture"/>
    </FILTER>
    <LANGINFOS>
      <LANGINFO lang="*" label="Pin"/>
    </LANGINFOS>
    <PROJECTS>
      <LOCAL name=".">
        <SOURCES>
          <FOLDER name="dealer_map" store="mediastore"/>
        </SOURCES>
      </LOCAL>
    </PROJECTS>
  </FS_REFERENCE>

  <FS_REFERENCE name="tt_pin_selected" hFill="yes" useLanguages="no">
    <FILTER>
      <ALLOW type="picture"/>
    </FILTER>
    <LANGINFOS>
      <LANGINFO lang="*" label="Pin Selected"/>
    </LANGINFOS>
    <PROJECTS>
      <LOCAL name=".">
        <SOURCES>
          <FOLDER name="dealer_map" store="mediastore"/>
        </SOURCES>
      </LOCAL>
    </PROJECTS>
  </FS_REFERENCE>

</CMS_MODULE>